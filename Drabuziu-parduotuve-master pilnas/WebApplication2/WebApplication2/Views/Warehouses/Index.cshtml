@model IEnumerable<WebApplication2.Models.Warehouse>

@{
    ViewBag.Title = "Sandėliai";
}

<h2>Sandėliai</h2>

<p>
    @Html.ActionLink("Sukurti naują", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Pavadinimas
        </th>
        <th>
            Vieta
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.place)
            </td>
            <td>
                @Html.ActionLink("Redaguoti", "Edit", new { id = item.id }) |
                @Html.ActionLink("Išsamiau", "Details", new { id = item.id }) |
                @Html.ActionLink("Ištrinti", "Delete", new { id = item.id })
            </td>
        </tr>
    }

</table>
